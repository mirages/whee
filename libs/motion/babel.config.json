{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "absoluteRuntime": false,
        "corejs": { 
          "version": 3,
          "proposals": true
        },
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ]
  ]
}